{
  "format": 1,
  "license": {
    "type": "MIT",
    "url": "https://www.oandgexample.com/license/"
  },
  "developer": {
    "authors": []
  },
  "application": {
    "type": "scheduler",
    "key": "provider.gamma-depth-scheduler",
    "visibility": "private",
    "name": "Gamma Depth Scheduler",
    "category": "analytics",
    "segments": [
      "drilling"
    ]
  },
  "settings": {
    "entrypoint": {
      "file": "src/gamma_depth",
      "function": "gamma_depth"
    },
    "timeout": 120,
    "memory": 128,
    "runtime": "python3.8",
    "app": {
      "cron_string": "*/5 * * * *"
    }
  },
  "environment": {},
  "datasets": {
    "provider.actual-gamma-depth": {
      "permissions": [
        "read",
        "write"
      ]
    },
    "corva.data.drillstring": {
      "permissions": [
        "read"
      ]
    },
    "corva.wits": {
      "permissions": [
        "read"
      ]
    }
  }
}
